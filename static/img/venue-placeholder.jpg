<?xml version="1.0" encoding="UTF-8"?>
<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="gold" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#FFD700" />
      <stop offset="100%" stop-color="#FFA500" />
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="8" />
    </filter>
  </defs>
  
  <!-- Coin shadow -->
  <circle cx="256" cy="256" r="240" fill="#00000033" filter="url(#shadow)" />
  
  <!-- Coin base -->
  <circle cx="256" cy="256" r="240" fill="url(#gold)" stroke="#B8860B" stroke-width="8" />
  
  <!-- Coin edge shading -->
  <circle cx="256" cy="256" r="240" fill="none" stroke="#FFC107" stroke-width="16" stroke-opacity="0.5" />
  
  <!-- Coin highlight -->
  <ellipse cx="200" cy="170" rx="180" ry="160" fill="#FFFFFF20" />
  
  <!-- Coin symbol -->
  <text x="256" y="300" font-size="160" font-weight="bold" text-anchor="middle" fill="#B8860B">$</text>
</svg> 